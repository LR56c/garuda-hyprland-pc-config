{
    "pyprland": {
      "plugins": [
        "scratchpads",
        "lost_windows",
        "workspaces_follow_focus"
      ]
    },
    "scratchpads": {
      "volume": {
        "command": "pavucontrol",
        "unfocus": "hide",
        "animation": "fromRight"
      }, 
      "discord": {
        "command": "discord",
        "unfocus": "hide",
        "animation": "fromBottom",
        "offset": "500"
      },
      "gimp": {
        "command": "gimp",
        "unfocus": "hide",
        "animation": "fromTop"
      }
    },
    "workspaces_follow_focus": {
      "max_workspaces": 2
    }
  }